# OCR SETTINGS
# â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€

# Minimum confidence threshold for Tesseract OCR results (0-100)
# Default: 60
# Impact: Lower = more text extracted but potentially less accurate
#         Higher = only high-confidence text but may miss some words
VITE_OCR_MIN_CONFIDENCE=60

# Minimum text length to consider valid OCR result
# Default: 1
# Impact: Filters out very short OCR results that may be noise
VITE_OCR_MIN_TEXT_LENGTH=1

